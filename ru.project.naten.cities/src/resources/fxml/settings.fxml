<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="16.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_RIGHT" disable="true" spacing="16.0">
         <children>
            <Label text="Language" HBox.hgrow="NEVER" />
            <Pane HBox.hgrow="ALWAYS" />
            <ChoiceBox prefWidth="150.0" HBox.hgrow="NEVER" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </HBox>
      <Separator prefWidth="200.0" />
      <VBox spacing="8.0">
         <children>
            <Label text="Game Time" />
            <HBox alignment="CENTER_RIGHT" spacing="8.0">
               <children>
                  <Label fx:id="gameTimeNum" text="300" HBox.hgrow="NEVER" />
                  <Pane HBox.hgrow="ALWAYS" />
                  <Slider fx:id="gameTimeSlider" majorTickUnit="60.0" max="601.0" min="20.0" minorTickCount="30" showTickMarks="true" value="300.0" HBox.hgrow="NEVER" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
      <Separator prefWidth="200.0" />
      <VBox fx:id="alertTimePanel" spacing="8.0">
         <children>
            <Label text="Alert Time" />
            <HBox alignment="CENTER_RIGHT" spacing="8.0">
               <children>
                  <Label fx:id="alertTimeNum" text="50" HBox.hgrow="NEVER" />
                  <Pane HBox.hgrow="ALWAYS" />
                  <Slider fx:id="alertTimeSlider" majorTickUnit="60.0" minorTickCount="5" showTickMarks="true" value="50.0" HBox.hgrow="NEVER" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER_LEFT" spacing="8.0">
         <children>
            <Label text="Alert" HBox.hgrow="NEVER" />
            <Pane HBox.hgrow="ALWAYS" />
            <CheckBox fx:id="alert" alignment="CENTER_RIGHT" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false" onAction="#alertChoise" selected="true" HBox.hgrow="NEVER" />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </HBox>
      <Separator prefWidth="200.0" />
      <HBox spacing="8.0">
         <children>
            <Label text="Check Words" />
            <Pane HBox.hgrow="ALWAYS" />
            <CheckBox fx:id="checkWord" mnemonicParsing="false" onAction="#checkWordsChoise" selected="true" HBox.hgrow="NEVER" />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
